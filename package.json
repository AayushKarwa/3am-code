{
  "name": "3am-code-by-ak",
  "displayName": "3am Code",
  "description": "A sleek, high-contrast dark theme optimized for late-night coding sessions.",
  "publisher": "AayushKarwa",
  "version": "0.0.8",
  "license": "MIT",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Themes"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AayushKarwa/3am-code"
  },
  "homepage": "https://github.com/AayushKarwa/3am-code",
  "bugs": {
    "url": "https://github.com/AayushKarwa/3am-code/issues"
  },
  "icon": "images/icon.jpg",
  "galleryBanner": {
    "color": "#1E1E1E",
    "theme": "dark"
  },
  "keywords": [
    "dark-theme",
    "night-mode",
    "developer-theme",
    "vscode-theme",
    "3am-code"
  ],
  "contributes": {
    "themes": [
      {
        "label": "3am Code Dark no italics",
        "uiTheme": "vs-dark",
        "path": "./themes/3am-code-dark-noitalics.json"
      },
      {
        "label": "3am Code Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/3am-code-color-theme.json"
      },
      {
        "label": "3am Code Light",
        "uiTheme": "vs",
        "path": "./themes/3am-code-light-color-theme.json"
      },
      {
        "label": "3am Code Light no italics",
        "uiTheme": "vs",
        "path": "./themes/3am-code-light-noitalics.json"
      }
     
    ]
  },
  "gallery": {
    "images": [
      "images/screenshot1.png",
      "images/screenshot2.png"
    ]
  },
  "scripts": {
    "publish": "vsce publish",
    "package": "vsce package"
  },
  "devDependencies": {
    "vsce": "^2.14.0"
  }
}
